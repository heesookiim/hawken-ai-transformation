{
  "name": "ai-transformation-plan-generator",
  "version": "1.0.0",
  "description": "AI Transformation Plan Generator for company to see how they can improve their business using AI",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server-dev\" \"npm run ui-dev\"",
    "server-dev": "cd server && npm run dev",
    "ui-dev": "cd ui && npm run dev",
    "start": "concurrently \"npm run server\" \"npm run ui\"",
    "server": "cd server && npm start",
    "ui": "cd ui && npm start",
    "install-all": "npm install && npm run install-server && npm run install-ui",
    "install-server": "cd server && npm install",
    "install-ui": "cd ui && npm install",
    "build": "npm run build-server && npm run build-ui",
    "build-server": "cd server && npm run build",
    "build-ui": "cd ui && npx next build"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "concurrently": "^7.6.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@types/node": "^22.13.10",
    "axios": "^1.8.4",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.3.0",
    "@types/fs-extra": "^11.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
